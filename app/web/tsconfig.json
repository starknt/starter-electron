{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": false,
    "skipLibCheck": true,
    "types": [],
    "baseUrl": ".",
    "paths": {
      "#common": [
        "../compat/preload/common.ts"
      ],
      "#preload/common": [
        "../compat/preload/common.ts"
      ],
      "#preload/test1": [
        "../compat/preload/test1.ts"
      ],
      "@app/compat-common": [
        "../compat/common/index.ts"
      ],
      "@app/compat-browser": [
        "../compat/browser/index.ts"
      ],
      "@app/compat-node": [
        "../compat/node/index.ts"
      ],
      "@app/compat-i18n": [
        "../compat/i18n/index.ts"
      ]
    }
  },
  "exclude": [
    "tests",
    "vite.config.ts"
  ]
}
