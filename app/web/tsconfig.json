{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": false,
    "skipLibCheck": true,
    "types": [
    ],
    "baseUrl": ".",
    "paths": {
      "@starter/shared": [
        "../../packages/shared/src/index.ts"
      ]
    }
  },
  "exclude": [
    "tests",
    "vite.config.ts"
  ]
}
